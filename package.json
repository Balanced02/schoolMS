{
  "name": "schoolms",
  "version": "0.0.1",
  "description": "Use the latest JavaScript features with Node, Today",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheHashHub/node-express-boilerplate.git"
  },
  "author": "Ayomide Daniel <adebalanced02@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "8.6.0",
    "npm": ">=5.5.1"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "concurrently \"node dist/index.js --exec babael-node\" \"cd client && npm run build\" ",
    "dev": "concurrently \"nodemon server/index.js --exec babel-node\" \"cd client && npm run dev\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel server -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w server test",
    "ready": "npm install && cd client && npm install && cd .."
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "body-parser": "^1.18.2",
    "cloudant": "^1.10.0",
    "concurrently": "^3.5.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "couchbase": "^2.4.3",
    "crypto": "^1.0.1",
    "cuid": "^1.3.8",
    "dotenv": "^4.0.0",
    "dropbox": "^2.5.12",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-session": "^1.15.5",
    "moment": "^2.18.1",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-fetch": "^1.7.2",
    "nodemon": "^1.11.0",
    "ottoman": "^1.0.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.2.1",
    "react": "^15.6.2",
    "react-day-picker": "^6.1.1",
    "react-dropzone": "^4.2.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "superagent": "^3.8.1",
    "supertest": "^3.0.0",
    "uuid": "^3.1.0",
    "validator": "^8.0.0",
    "webpack-dev-server": "^2.9.7",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack": "^3.6.0",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/TheHashHub/node-express-boilerplate/issues"
  },
  "homepage": "https://github.com/TheHashHub/node-express-boilerplate/#readme",
  "directories": {
    "test": "test"
  }
}
